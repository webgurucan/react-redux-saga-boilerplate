{
  "source": {
    "include": [
      "app/scripts"
    ],
    "includePattern": ".+\\.js(doc)?$"
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "template": "./node_modules/minami"
  },
  "plugins": ["node_modules/jsdoc-babel"],
  "babel": {
    "plugins": ["transform-object-rest-spread", "transform-flow-strip-types"]
  }
}
